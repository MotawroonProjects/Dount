   }\r\n\r\n                    @Override\r\n                    public void onError(@NonNull Throwable throwable) {\r\n\r\n                    }\r\n                });\r\n            }\r\n        });\r\n\r\n\r\n    }\r\n\r\n\r\n    @Override\r\n    protected void onCleared() {\r\n        super.onCleared();\r\n        disposable.clear();\r\n    }\r\n}\r\n
